<link rel="stylesheet" href="/stylesheets/new.css">

<main>
  <h1><%= title %></h1>
  <br>
  <form action="/songs" method="POST" autocomplete="off">
    <div class="form-group">
      <label>Song Title</label>
      <input type="text" class="form-control" name="title" placeholder="Enter Song Title" style="width: 30rem;">
    </div>
    <div class="form-group">
      <label>Artist</label>
      <input type="text" class="form-control" name="artist" placeholder="Artist Name">
    </div>
    <div class="form-group">
      <label>Song Link</label>
      <input type="text" class="form-control" name="link" placeholder="Song Link">
    </div>
    <button type="submit" class="btn btn-dark">Submit</button>
  </form>
